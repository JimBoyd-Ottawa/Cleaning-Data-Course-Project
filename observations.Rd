\name{observations}
\alias{observations}
\docType{data}
\title{
%% observations data frame description 
}
\description{
%% This observation data is a further analysis of data from an experiment run in Genoa, Italt
%% here a large number of experiments were run using smartphones the full description is in the link below:
%%  http://archive.ics.uci.edu/ml/datasets/Human+Activity+Recognition+Using+Smartphones
%% The raw source data was downloaded from the link below:
%% https://d396qusza40orc.cloudfront.net/getdata%2Fprojectfiles%2FUCI%20HAR%20Dataset.zip
%% all of the data was summarized by converting the 128 readings over time into a mean and a standard deviation
}
\usage{data("observations")}
\format{
  A data frame with 10299 observations on the following 90 variables.
  \describe{
    \item{\code{observationid}}{an integer vector - This contains the sequential observation number}
    \item{\code{subjectid}}{an integer vector - This contains the number assigned to the 30 subjects in the study}
    \item{\code{activitydesc}} {a factor with 6 values representling the type of activity being tested}
    \item{\code{set}} {a factor with 2 values representing whether the observation was chosen for the test or train set}
    \item{\code{timebodyaccelerometer-mean-x}} (a numeric vector indicating the mean accelerations in the experiment in the x direction in units of Gravity "g's")
    \item{\code{timebodyaccelerometer-mean-y}} (a numeric vector indicating the mean acceleration in the experiment in the y direction in units of Gravity "g's")
    \item{\code{timebodyaccelerometer-mean-z}} (a numeric vector indicating the mean acceleration in the experiment in the z direction in units of Gravity "g's")
    \item{\code{timegravityaccelerometer-mean-x}} (a numeric vector indicating the mean acceleration in the experiment in the x direction in units of Gravity "g's")
    \item{\code{timegravityaccelerometer-mean-y}} (a numeric vector indicating the mean acceleration in the experiment in the y direction in units of Gravity "g's")
    \item{\code{timegravityaccelerometer-mean-z}} (a numeric vector indicating the mean acceleration in the experiment in the z direction in units of Gravity "g's")
    \item{\code{timebodyaccelerometerjerk-mean-x}} (a numeric vector indicating the mean accelerations in the experiment in the x direction in units of Gravity "g's")  
    \item{\code{timebodyaccelerometerjerk-mean-y}} (a numeric vector indicating the mean accelerations in the experiment in the y direction in units of Gravity "g's") 
    \item{\code{timebodyaccelerometerjerk-mean-z}} (a numeric vector indicating the mean acceleration in the experiment in the z direction in units of Gravity "g's")
    \item{\code{timebodygyroscope-mean-x}} (a numeric vector indicating the mean circular velocity in the experiment in the x direction in units of radians/second)
    \item{\code{timebodygyroscope-mean-y}} (a numeric vector indicating the mean circular velocity in the experiment in the y direction in units of radians/second)
    \item{\code{timebodygyroscope-mean-z}} (a numeric vector indicating the mean circular velocity in the experiment in the z direction in units of radians/second)
    \item{\code{timebodygyroscopejerk-mean-x}} (a numeric vector indicating the mean circular velocity in the experiment in the x direction in units of radians/second)
    \item{\code{timebodygyroscopejerk-mean-y}} (a numeric vector indicating the mean circular velocity in the experiment in the y direction in units of radians/second)
    \item{\code{timebodygyroscopejerk-mean-z}} (a numeric vector indicating the mean circular velocity in the experiment in the z direction in units of radians/second)
  }
}
